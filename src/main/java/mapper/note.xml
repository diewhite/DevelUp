<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="com.multi.ongo.note">
  	<select id="findbyid" resultType="note" parameterType="String">
  		select * from note where receive_id = #{id}
  	</select>
  	<select id="receivelist" resultType="note" parameterType="String">
  		select * from note where receive_id = #{id}
  	</select>
  	<select id="sendlist" resultType="note" parameterType="String">
  		select * from note where send_id = #{id}
  	</select>
  </mapper>
  