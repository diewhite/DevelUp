 		<put-attribute name="content" value="/WEB-INF/history/dealHistoryList.jsp"></put-attribute>
 	</definition>
 	
 	<!-- 중고거래내역 상세 페이지 -->
	<definition name="history/dealDetail" extends="indexTemplate">
 		<put-attribute name="content" value="/WEB-INF/history/historyDetail.jsp"></put-attribute>
 	</definition> 	
 	
